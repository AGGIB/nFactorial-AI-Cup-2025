{
  "version": 2,
  "builds": [
    {
      "src": "backend/dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/dist/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "installCommand": "cd backend && npm ci && npx prisma generate"
} 